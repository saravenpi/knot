  "query": "\n        SELECT p.id, p.owner_id, p.team_id,\n               CASE WHEN tm.role IS NOT NULL THEN tm.role ELSE NULL END as team_role\n        FROM packages p\n        LEFT JOIN team_members tm ON p.team_id = tm.team_id AND tm.user_id = $3\n        WHERE p.name = $1 AND p.version = $2\n        ",
